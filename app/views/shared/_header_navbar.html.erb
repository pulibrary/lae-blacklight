<header role="banner" class="wrapper--branding">
  <div  class="l-branding__pul">
    <div class="wrapper">
       <a href="http://library.princeton.edu" title="Princeton University Library" rel="home" class="logo__pul">
        <%= image_tag("library_full_logo.svg", class: "logo-svg") %>
        <%= image_tag("library_full_logo.png", class: "logo-no-svg") %>
      </a>
    </div>
  </div>
  <div class="l-branding__site">
    <div class="wrapper">
      <%= link_to(root_path) do %>
        <%= image_tag("dalace_logo.png", class: "logo__site logo-no-svg") %>
        <%= image_tag("dalace_logo.png", class: "logo__site logo-svg") %>
      <% end %>
      <h2>
        <%= link_to(root_path) do %>
          <%= application_name %>
        <% end %>
      </h2>

      <div id="user-util-collapse" class="site-links">
        <%= render partial: '/shared/lae_search_pages' %>
      </div>

      <div class="browse">
        <%= link_to 'Browse', page_path('catalog', sort: 'date_modified desc', search_field:'all_fields') %>
       </div>
        
      <div class="search-bar">
        <%= render_search_bar  %> 
      </div>

      <div id="user-util-collapse" class="search-links">
        <%= render partial: '/user_util_links' %>
      </div>
    </div>
  </div>
</header>
